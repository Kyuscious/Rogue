{
  "name": "riot-roguelike",
  "version": "0.1.0",
  "description": "A roguelike game featuring characters from the Riot universe",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && cd backend && npm install && npm run build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "start": "node server.js",
    "start:backend": "cd backend && npm start"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.50.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
